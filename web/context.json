{
   "@context":{
      "as":"http://www.w3.org/ns/activitystreams#",
      "xsd":"http://www.w3.org/2001/XMLSchema#",
      "dcterms":"http://purl.org/dc/terms/",
      "prov":"https://www.w3.org/TR/prov/#",
      "foaf":"http://xmlns.com/foaf/spec/#term_",
      "oa" : "http://www.w3.org/ns/oa#",
      "rerum_v1":"https://github.com/CenterForDigitalHumanities/rerum_server/blob/master/API.md#",
      "id":"@id",
      "type":"@type",
      "releasing" : {
          "@id":"rerum_v1:releasing",
          "@type":"oa:Motivation"
      },
      "working" : {
          "@id":"rerum_v1:releasing",
          "@type":"oa:Motivation"
      },
      "__rerum":{
          "@id" : "rerum_v1:__rerum",
          "@container" : "@graph"
      },
      "Agent":{
         "@id":"foaf:Agent",
         "@type":"@id"
      },
      "alpha":{
         "@id":"rerum_v1:alpha",
         "@type":"xsd:boolean"
      },
      "createdAt":{
         "@id":"dcterms:created",
         "@type":"xsd:dateTime"
      },
      "generatedBy":{
         "@id":"as:generator",
         "@type":"@id"
      },
      "isReleased":{
         "@id":"dcterms:issued",
         "@type":"xsd:dateTime"
      },
      "isOverwritten":{
         "@id":"rerum_v1:isOverwritten",
         "@type":"xsd:dateTime"
      },
      "releases":{
         "@id":"rerum_v1:releases",
         "container" : "@graph",
         "@context":{
            "previous":{
               "@id":"rerum_v1:releases.previous",
               "@type":"@id"
            },
            "next":{
               "@id":"rerum_v1:releases.next",
               "@container":["@set", "@id"]
            },
            "replaces":{
               "@id":"rerum_v1:releases.replaces",
               "@type":"@id"
            }
         }
      },
      "history":{
         "@id":"rerum_v1:history",
         "container" : "@graph",
         "@context":{
            "previous":{
               "@id":"prov:wasRevisionOf",
               "@type":"@id"
            },
            "next":{
               "@id":"dcterms:isVersionOf",
               "@container":["@set", "@id"]
            },
            "prime":{
               "@id":"rerum_v1:prime",
               "@type":"@id"
            }
         }
      },
      "APIversion":{
         "@id":"rerum_v1:APIversion",
         "@type" : "xsd:string"
      },
      "__deleted":{
            "@id" : "rerum_v1:delete",
            "@container" : "@graph",
            "@context":{
                "object":{
                    "@id" : "rerum_v1:object",
                    "@container" : "@graph"
                },
                "deletor":{
                    "@id" : "rerum_v1:deletor",
                    "@type" : "@id"
                },
                "time" : {
                    "@id" : "dcterms:date",
                    "@type":"xsd:time"
                }
            }
      }
   }
}
